#include<bits/stdc++.h>
using namespace std;
int main()
{
    int ar[12]={4,7,44,77,47,74,477,744,747,474,774,447};
    int n;
    cin>>n;
    for(int i=0;i<12;i++)
    {
       if(n==ar[i] || n%ar[i]==0 )
       {
           cout<<"YES"<<endl;
           return 0;
       }
 
 
    }
    cout<<"NO"<<endl;
 
    return 0;
 
}
