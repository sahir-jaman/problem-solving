#include<bits/stdc++.h>
using namespace std;
int a,b,c,d,p,q,x,y;
int main()
{
    int n;
    string s;
    cin>>n;
    cin>>s;
    for(int i=0;i<n;i++)
    {
        if(s[i]=='A')
        {
            c++;
        }
        else
        {
            p++;
        }
    }
    if(c>p)
    {
        cout<<"Anton";
    }
    else if(p>c)
    {
        cout<<"Danik";
    }
    else
    {
        cout<<"Friendship";
    }
    return 0;
}
