#include<bits/stdc++.h>
using namespace std;
int c,d,e,f;
int main()
{
    int n,h,a;
    vector<int>d;
    cin>>n>>h;
    for(int i=1; i<=n; i++)
    {
        cin>>f;

        d.push_back(f);
    }

    for(int i=0; i<d.size(); i++)
    {
        if(d[i]>h)
        {
            c=c+2;
        }
        else
        {
            e=e+1;
        }

    }
   int x=e+c;
    cout<<x;

    return 0;
}
